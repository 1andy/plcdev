@model SharedLayoutViewModel
<!DOCTYPE html>
<html>
<head>
    <title>
        @Model.PageTitle @(Model.PageTitle != null ? "-" : string.Empty)
        @Model.StoreName
    </title>
    <link href="@Url.Content("~/content/css/01-reset.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/content/css/02-tools.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/content/css/05-common.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/content/css/10-layout.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/scripts/01-jquery-1.5.1.min.js")"></script>
    <script src="@Url.Content("~/scripts/10-common.js")"></script>
    @RenderSection("head", false)
</head>
<body>
    <div id="page-content">
        @{Html.RenderPartial("Header");}
        @RenderBody()
        @{Html.RenderPartial("Footer");}
    </div>
</body>
</html>
